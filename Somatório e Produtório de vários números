#include <stdio.h>

int main() {
    int qtd, i, num, soma = 0, produto = 1;

    printf("Quantos números deseja digitar? ");
    scanf("%d", &qtd);

    for (i = 0; i < qtd; i++) {
        printf("Digite o número %d: ", i + 1);
        scanf("%d", &num);

        soma += num;
        produto *= num;
    }

    printf("Somatório: %d\n", soma);
    printf("Produtório: %d\n", produto);

    return 0;
}
